    <script>
      $(document).ready(function () {
        $('#search_param').keypress(function (event) {
          var keycode = (event.keyCode ? event.keyCode : event.which);
          if (keycode == '13') {
            reditect_url();
          }
        });
      });
      function reditect_url () {
        var keyword = $("#search_param").val();
        var url = 'http://photobucket.com/images/' + keyword;
        window.location = url;
      }
