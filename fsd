      <script>
        $(document).ready(function () {
          $('#newsletter').keypress(function (event) {
            var keycode = (event.keyCode ? event.keyCode : event.which);
            if (keycode == '13') {
              news_reditect_url();
            }
          });
        });
        function news_reditect_url () {
          $("#newsletter").removeAttr('style');
          var keyword = $("#newsletter").val();
          $("#newsletter_submit").attr('disabled', 'disabled');
          $("#newsletter_submit").attr('style', 'opacity: 0.5');
          var emailregx = /^(([^<>()\[\]\\.,;:~+=$\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
          var res = emailregx.test(keyword);
          if (keyword.search(/\S/) == -1)
          {
            $("#newsletter").attr('style', 'border: 1px solid red;');
            $("#newsletter_submit").removeAttr('disabled');
            $("#newsletter_submit").removeAttr('style');
          }
          else if (res === true)
          {
            $.ajax({
              url: '/admin/index.php?pages=newsletter_add&call_type=ajax',
              url: '/admin/index.php?pages=newsletter_add&call_type=ajax',
              data: {emailid: keyword},
              method: 'POST',
              success: function (response) {
                $("#newsletter").val('');
                $(".modal-body").html('<span class="success-check"><i class="fa fa-check-circle" aria-hidden="true"></i></span> Thank you for subscribing. Please be on the lookout for our daily membership offers.');
                $("#modal_button").click();
                $("#newsletter_submit").removeAttr('disabled');
                $("#newsletter_submit").removeAttr('style');
              }
            });
          }
          else {
            $("#newsletter").attr('style', 'border: 1px solid red;');
            $("#newsletter_submit").removeAttr('disabled');
            $("#newsletter_submit").removeAttr('style');
          }
        }
      </script>
      <div class="container">
        <h3>Join to get updates, deals, &amp; more!</h3>
        <div class="newsletter_row clearfix">
          <form name="newsletter_mail_form" onsubmit="return false" id="newsletter_mail_form" action="" method="post">
            <div class="newsletter_form pull-left clearfix">
              <input type="text" placeholder="Enter your email" name="newsletter" id="newsletter" class="newsletter_input" />
              <button id="newsletter_submit" type="button" onclick="news_reditect_url()" class="newsletter_btn">JOIN NOW</button>
            </div>
          </form>
